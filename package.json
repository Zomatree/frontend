{
  "name": "stoat-for-web",
  "version": "0.0.12",
  "version-date": "2025-10-10",
  "description": "Stoat for Web: frontend software for Stoat",
  "scripts": {
    "dev:web": "pnpm --filter client dev",
    "build:deps": "pnpm --filter revolt.js build && pnpm --filter solid-livekit-components build && pnpm --filter @lingui-solid/babel-plugin-lingui-macro build && pnpm --filter @lingui-solid/babel-plugin-extract-messages build && pnpm --filter client lingui:compile",
    "build:web": "pnpm --filter client build",
    "build:prod": "pnpm --filter client build:prod",
    "start:web": "cd packages/client && pnpm run serve",
    "lint": "eslint --fix --ext .ts,.tsx packages/client",
    "fmt": "prettier --write 'packages/client/**/*.{ts,tsx,json}'",
    "lingui": "pnpm --filter client lingui:compile && pnpm --filter client lingui:extract",
    "test:lint": "eslint --ext .ts,.tsx packages/client",
    "test:fmt": "prettier --check 'packages/client/**/*.{ts,tsx,json}'",
    "test:typecheck": "pnpm --filter client test:typecheck",
    "test:suite": "pnpm --filter client test:suite",
    "test:coverage": "pnpm --filter client test:coverage"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@pandabox/prettier-plugin": "^0.1.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "eslint": "^9.26.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-solid": "^0.14.5",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.32.0"
  },
  "keywords": [],
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "private": true,
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}